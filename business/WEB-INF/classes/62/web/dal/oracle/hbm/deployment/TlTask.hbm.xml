<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.lxt2.systemapp.model.deployment.TlTask" table="TL_TASK">

		<id name="numtaskid" type="java.lang.Integer">
			<column name="NUMTASKID" precision="22" scale="0" />
			<generator class="increment"></generator>
		</id>

		<property name="numprogramid" type="java.lang.Integer">
			<column name="NUMPROGRAMID" length="22" />
		</property>

		<property name="vc2taskname" type="java.lang.String">
			<column name="VC2TASKNAME" length="200" />
		</property>

		<property name="vc2taskdesc" type="java.lang.String">
			<column name="VC2TASKDESC" length="200" />
		</property>

	</class>
</hibernate-mapping>
  