<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.lxt2.systemapp.model.deployment.TlCommonItem"
		table="TL_COMMON_ITEM">

		<id name="numitemid" type="java.lang.Integer">
			<column name="NUMITEMID" precision="20" scale="0" />
			<generator class="increment"></generator>
		</id>

		<property name="numtypeid" type="java.lang.Integer">
			<column name="NUMTYPEID" length="20" />
		</property>

		<property name="vc2key" type="java.lang.String">
			<column name="VC2KEY" length="200" />
		</property>

		<property name="vc2value" type="java.lang.String">
			<column name="VC2VALUE" length="200" />
		</property>

		<property name="vc2name" type="java.lang.String">
			<column name="VC2NAME" length="200" />
		</property>

		<property name="vc2desc" type="java.lang.String">
			<column name="VC2DESC" length="200" />
		</property>

	</class>
</hibernate-mapping>
  