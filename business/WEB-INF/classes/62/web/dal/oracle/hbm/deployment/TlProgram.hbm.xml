<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.lxt2.systemapp.model.deployment.TlProgram"
		table="TL_PROGRAM" dynamic-insert="true" dynamic-update="true">
		<id name="numProgramId" type="java.lang.Integer">
			<column name="NUMPROGRAMID" precision="20" scale="0" />
			<generator class="increment"></generator>
		</id>

		<property name="numTypeId" type="java.lang.Integer">
			<column name="NUMTYPEID" length="20" />
		</property>

		<property name="vc2ProgramName" type="java.lang.String">
			<column name="VC2PROGRAMNAME" length="200" />
		</property>

		<property name="vc2ProgramDesc" type="java.lang.String">
			<column name="VC2PROGRAMDESC" length="200" />
		</property>
	</class>
</hibernate-mapping>
