<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.lxt2.systemapp.model.alert.TlAlertType" table="TL_ALERT_TYPE">

		<!--(NUMBER:22)-->
		<id name="numtypeid" type="java.lang.Integer">
			<column name="NUMTYPEID" precision="22" scale="0" />
			<generator class="increment"></generator>
		</id>

		<!--(VARCHAR2:200)-->
		<property name="numtypename" type="java.lang.String">
			<column name="NUMTYPENAME" length="200" />
		</property>

	</class>
</hibernate-mapping>
  