<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- 指标bll。 -->
	<bean id="TlKpiValueBLL" class="com.lxt2.systemapp.bll.monitor.TlKpiValueBLL"
		init-method="main" scope="prototype">
	</bean>

	<!-- 公式定义bll。 -->
	<bean id="TlKpiDefBLL" class="com.lxt2.systemapp.bll.monitor.TlKpiDefBLL"
		init-method="main" scope="prototype">
	</bean>

	<!-- 告警数据bll。 -->
	<bean id="TlAlarmDataBLL" class="com.lxt2.systemapp.bll.monitor.TlAlarmDataBLL"
		init-method="main" scope="prototype">
	</bean>
</beans>